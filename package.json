{
  "name": "@koicarp/easypage",
  "version": "1.0.20",
  "description": "一款轻量级的mvvm前端框架，支持es6 class 的面向对象方式进行开发，让你从繁琐的dom操作中解放，并提高代码的可读性，支持IE9+以上浏览器。",
  "main": "dist/index.js",
  "files": [
    "src",
    "style.css",
    "dist/*.js"
  ],
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack --config webpack.config.js -w",
    "build": "cross-env NODE_ENV=production webpack --config webpack.config.js",
    "analyz": "webpack-bundle-analyzer --port 7777 ./dist/_bundle_stats.json",
    "lint": "eslint ."
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/baronhuang/easypage.git"
  },
  "author": "baronhuang",
  "license": "MIT",
  "dependencies": {
    "@babel/core": "^7.8.7",
    "@babel/runtime": "^7.8.7",
    "core-js": "^3.2.1"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.8.7",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "cross-env": "^5.0.1",
    "css-loader": "^3.5.0",
    "eslint": "^5.16.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-formatter-friendly": "^6.0.0",
    "eslint-loader": "2.2.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "style-loader": "^1.1.3",
    "terser-webpack-plugin": "^1.4.0",
    "webpack": "^4.20.2",
    "webpack-bundle-analyzer": "^3.6.1",
    "webpack-cli": "^3.3.1",
    "webstorm-disable-index": "^1.2.0"
  }
}
